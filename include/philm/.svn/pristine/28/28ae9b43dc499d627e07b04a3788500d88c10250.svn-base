function prompter_showhelp
%function prompter_showhelp

%should get version currently running and eliminate unneeded commands

helpstr=str2mat(...
    'Commands are:',...
    'n = next (default)',...
	'a = autonext',...
	't = set trial number',...
    'c = comment',...
    'd = set trial duration',...
    'r = repeat',...
    'R = repeat with old trial number',...
    'x = mark',...
    'X = show marked stimuli',...
    'g = goto',...
    'm = message',...
    'M = message and resume',...
    'q = quit',...
    'k = keyboard',...
    'h = help');
disp(helpstr);
helpwin(helpstr,'Prompt commands');

